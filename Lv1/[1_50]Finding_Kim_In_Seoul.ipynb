      "source": [
        "def solution(seoul):\n",
        "    temp = 0\n",
        "    for i, s in enumerate(seoul):\n",
        "        if s == 'Kim':\n",
        "            temp = i\n",
        "            break\n",
        "    answer = '김서방은 ' + str(temp) + '에 있다'\n",
        "    return answer"
