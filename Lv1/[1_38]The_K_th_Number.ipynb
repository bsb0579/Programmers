      "source": [
        "def solution(array, commands):\n",
        "    answer = []\n",
        "    for a, b, c in commands:\n",
        "        temp = array[a-1:b]\n",
        "        temp.sort()\n",
        "        answer.append(temp[c-1])\n",
        "    return answer"
      ],
