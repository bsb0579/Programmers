      }
    },
    {
      "cell_type": "code",
      "source": [
        "def solution(s):\n",
        "    lst = []\n",
        "    for w in s:\n",
        "        lst.append(ord(w))\n",
        "    lst.sort(reverse = True)\n",
        "    answer = ''\n",
        "    for l in lst:\n",
        "        answer += chr(l)\n",
        "    return answer"
      ],
