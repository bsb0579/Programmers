      "source": [
        "def solution(s):\n",
        "    l = len(s)\n",
        "    if l == 0:\n",
        "        return ''\n",
        "    if l%2 == 1:\n",
        "        answer = s[l//2]\n",
        "    else:\n",
        "        answer = s[l//2-1:l//2+1]\n",
        "    return answer"
      ],
